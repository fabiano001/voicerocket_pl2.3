<h1>Listing voice_campaigns</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voice_campaigns.each do |voice_campaign| %>
      <tr>
        <td><%= voice_campaign.name %></td>
        <td><%= voice_campaign.user %></td>
        <td><%= link_to 'Show', voice_campaign %></td>
        <td><%= link_to 'Edit', edit_voice_campaign_path(voice_campaign) %></td>
        <td><%= link_to 'Destroy', voice_campaign, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voice campaign', new_voice_campaign_path %>
