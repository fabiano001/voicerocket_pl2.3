<%= simple_form_for(@contact_transaction) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :phone_numer %>
    <%= f.input :genre %>
    <%= f.input :duration %>
    <%= f.input :charge %>
    <%= f.association :run %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
